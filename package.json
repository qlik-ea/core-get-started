    "ws": "1.1.1"
  },
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-loader": "6.4.1",
    "babel-preset-env": "1.6.1",
    "copy-webpack-plugin": "4.2.0",
    "eslint": "4.10.0",
    "eslint-config-airbnb-base": "12.1.0",
    "eslint-plugin-import": "2.8.0",
    "json-loader": "0.5.7",
    "raw-loader": "0.5.1",
    "webpack": "1.15.0",
    "webpack-dev-server": "1.16.5"
  }
